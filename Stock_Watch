# import and graph stock price https://www.youtube.com/watch?v=0e-lsstqCdY

import datetime as dt
import matplotlib.pyplot as plt
from matplotlib import style
import pandas as pd
import `.data as web

#style.use('bmh')

start = dt.datetime(2007,1,1)
end = dt.datetime(2019,12,31)

df = web.DataReader('JPM','yahoo',start,end)
df2 = web.DataReader("WFC",'yahoo',start,end)
df3 = web.DataReader("C",'yahoo',start,end)
df4 = web.DataReader("USB",'yahoo',start,end)
df5 = web.DataReader("USB",'yahoo',start,end)
df6 = web.DataReader("BAC",'yahoo',start,end)
# print(df.head(5)) #shows first 5
# print(df.tail(5)) #shows last 5
df.Close.plot(figsize=(16, 8),label="JP Morgan", legend=True) #plots the close price
df2.Close.plot(label="Wells Fargo", legend=True)
df3.Close.plot(label="Citibank", legend=True)
df4.Close.plot(label="US Bank", legend=True)
df5.Close.plot(label="PNC", legend=True)
df6.Close.plot(label="B of A", legend=True)
plt.show()
